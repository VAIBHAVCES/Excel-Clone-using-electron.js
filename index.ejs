<!DOCTYPE html>

<html>

<head>

    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%=title%></title>
    <link rel="stylesheet" href="style.css">
    
</head>
<body>
<!-- <h1>First electron app</h1> -->

        <div class="menu-container" id="menu-container">
            <div id="File" class="menu">File</div>
            <div id="Home" class="menu">Home</div>
</div>
        <div class="menu-option-container" id="menu-option-container">
            <div id="File-menu-options" class="menu-options">
                <div id="new">New</div>
                <div id="open">Open</div>
                <div id="save">Save</div>
            </div>

        </div>

        <div class="formula-container" id="formula-container">
            <input type="address-input">
            <input type="formulae-input">
      </div>
    <div class="cell-container"> 
        <div class="tl-cell" id="tl-cell"></div>
        <div id="top-row" class="top-row">
            <%for(let i=0;i<cols;i++){%> 
                <div class="cell">
                    <%=String.fromCharCode(i+65)%>
                </div>
                
                
                <%}%>>

        </div>
        <div id="left-col" class="left-col">
            <%for(let i=0;i<rows;i++){%>
                <div class="cell"><%=i+1%></div> 
                <%}%>
        </div>
        <div id="grid" class="grid">
            <%for(let i=0;i<rows;i++){%>
                <div class="row">
                    <%for(let j=0;j<cols;j++){%>
                        <div class="cell" contenteditable="true" rid=<%=i%> cid=<%=j%>>cell</div>
                        
                        <%}%>

                </div>
            <%}%>

        </div>
    
    </div>
    <div id="sheets" class="sheets">Sheets</div>
    <script src="index.js"></script>


</body>
</html>